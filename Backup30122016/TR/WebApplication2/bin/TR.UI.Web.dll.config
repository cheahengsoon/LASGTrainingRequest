    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.5.2"/>
  </system.web>
  <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="basicHttpTrainingService" />
                <binding name="basicHttpTrainingWorkflowService1" />
