    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="AuditEngine" Order="1" Enabled="False">
          <Destination Path="Data Source=ENGSOON;Initial Catalog=HRTrainingRequest;Integrated Security=True" />
