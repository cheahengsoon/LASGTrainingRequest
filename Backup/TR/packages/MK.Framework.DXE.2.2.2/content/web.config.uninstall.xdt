<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <sectionGroup name="macrokiosk">
      <section name="dataExport" xdt:Locator="Match(name)" xdt:Transform="Remove" />
    </sectionGroup>
	<sectionGroup name="macrokiosk" xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)" />
	
  </configSections>
  <configSections xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
  
  <macrokiosk>
    <dataExport xdt:Transform="Remove" />
  </macrokiosk>
  <macrokiosk xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
  
  <system.serviceModel>
    <bindings>
        <netMsmqBinding>
            <binding name="netMsmqExecutionService" xdt:Locator="Match(name)" xdt:Transform="Remove" />
        </netMsmqBinding>
        <netMsmqBinding xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
        
    </bindings>
    <bindings xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
    
    <client>
        <endpoint name="netMsmqExecutionService" xdt:Locator="Match(name)" xdt:Transform="Remove" />
    </client>
    <client xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
    
  </system.serviceModel>
  <system.serviceModel xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
  
</configuration>