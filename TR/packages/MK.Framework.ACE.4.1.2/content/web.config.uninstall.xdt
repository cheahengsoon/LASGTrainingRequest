<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <sectionGroup name="macrokiosk">
      <section name="accessControl" xdt:Locator="Match(name)" xdt:Transform="Remove" />
    </sectionGroup>
	<sectionGroup name="macrokiosk" xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)" />
	
  </configSections>
  <configSections xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
  
  <macrokiosk>
    <accessControl xdt:Transform="Remove" />
  </macrokiosk>
  <macrokiosk xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />
	
</configuration>